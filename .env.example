# API Authentication
API_SHARED_TOKEN=your_secure_api_token_here
CORS_ORIGIN=http://localhost:5173
VITE_401K_TOKEN=your_secure_api_token_here

# Plaid API Configuration
# Get your credentials from https://dashboard.plaid.com/team/keys
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENV=production
PLAID_PRODUCTS=investments
PLAID_COUNTRY_CODES=US
VITE_PLAID_ENV=production
VITE_PLAID_ACCESS_PASSWORD=your_access_password

# Plaid Token Encryption
# Generate a secure 32-byte key: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
PLAID_TOKEN_ENCRYPTION_KEY=your_32_byte_base64_encryption_key_here

# Plaid Sandbox (for testing)
PLAID_SANDBOX_SECRET=your_sandbox_secret

# Supabase Configuration
# Get your credentials from https://app.supabase.com/project/_/settings/api
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Discord Webhook (optional - for webhook notifications)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN