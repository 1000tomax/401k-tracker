{
  "name": "401k-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && node scripts/analyze-bundle.js",
    "preview": "vite preview",
    "deploy": "npm run build && export $(cat .env | xargs) && wrangler pages deploy dist --project-name=401k-tracker",
    "perf:analyze": "node scripts/analyze-bundle.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "node-cron": "^4.2.1",
    "plaid": "^38.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-plaid-link": "^4.1.1",
    "react-router-dom": "^6.23.1",
    "recharts": "^2.12.5",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "msw": "^2.11.6",
    "rollup-plugin-visualizer": "^6.0.4",
    "sharp": "^0.34.4",
    "vite": "^7.1.6",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^4.0.7"
  }
